{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "25mb",
        "includeFiles": ["chainlit.md", "public/**", "*.db"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py",
      "methods": ["GET", "POST", "WS"] 
    }
  ]
}